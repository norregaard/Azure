          "value": false
      },
      "ASN": {
          "value": "<insert ASN number>"
      },
      "primaryPeerAddPrefix": {
          "value": "10.XXX.XXX.XXX/30"
      },
      "secondaryPeerAddPrefix": {
          "value": "10.XXX.XXX.XXX/30"
      },
      "vlan": {
          "value": 90
      },
      "enableReadOnlyLock": {
          "value": true
      },
      "gwManagerEtag": {
          "value": "7"
      }
  }
